mixin back-link(href, title)
  a.unflashy(href=href, title=title)
    svg.back-link-icon(large)
      use(xlink:href="#icon-arrow-back")

  svg.svg-reference(xmlns="http://www.w3.org/2000/svg")
    symbol#icon-arrow-back(viewBox="0 0 24 24")
      title Go back to the selection of Fusiontables to export
      path(d="M0 0h24v24H0z", fill="none")
      path(d="M20 11H7.83l5.59-5.59L12 4l-8 8 8 8 1.41-1.41L7.83 13H20v-2z" fill="currentColor")
